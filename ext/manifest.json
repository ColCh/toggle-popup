{
  "name": "Toggle PopUp",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Toggles popup mode for window",
  "icons": {
    "32": "icon-32.png",
    "128": "icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "dist/background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "tabs"
  ]
}
